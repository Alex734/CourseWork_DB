<div class="catalog_tit">
    <h1>Предложения недели</h1>
</div>

<div class="all_tovar" >
{{#each autos}}
    <div class="tovar" id={{idAuto}}>
        <a class="tov_img" href="">
            <img class="tov-img"
                src="http://localhost:3000/img/{{img}}.jpeg"
                alt="">
        </a>
        <a class="tov_info" href="http://localhost:3000/page/{{idAuto}}">
            <strong>{{Make}} {{Model}}</strong>
            <p>
                <span class="g_hide">{{VehicleType}}</span><br>
                <span> {{Year}} </span>
            </p>
            <div class="tov_price">{{Price}} р.</span></div>
        </a>
    </div>
{{/each}}
</div>
